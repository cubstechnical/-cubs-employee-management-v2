{
  "name": "cubs-admin",
  "version": "2.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "build:pwa": "next build",
    "build:analyze": "ANALYZE=true npm run build",
    "analyze:bundle": "node scripts/analyze-bundle.js",
    "performance:monitor": "node -e \"require('./lib/monitoring/performance').initializePerformanceMonitoring()\"",
    "db:migrate": "supabase db push",
    "db:indexes": "supabase db push --include-all",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:all": "jest",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:ci": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:comprehensive": "node scripts/test-runner.js",
    "build:mobile": "node scripts/build-mobile.js",
    "build:mobile:android": "npm run build:mobile && npx cap open android",
    "build:mobile:ios": "npm run build:mobile && cap open ios",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "cap": "cap",
    "cap:add:ios": "cap add ios",
    "cap:add:android": "cap add android",
    "cap:build": "npm run build:mobile",
    "cap:ios": "cap open ios",
    "cap:android": "cap open android",
    "android:build": "npm run cap:build && cd android && ./gradlew assembleDebug",
    "android:run": "npm run cap:build && cd android && ./gradlew runDebug",
    "ios:build": "npm run cap:build && cap open ios",
    "eas:build": "eas build",
    "eas:build:ios": "eas build --platform ios",
    "eas:build:android": "eas build --platform android",
    "eas:build:ios:production": "eas build --platform ios --profile production",
    "eas:build:android:production": "eas build --platform android --profile production",
    "eas:build:preview": "eas build --profile preview",
    "eas:submit": "eas submit",
    "eas:submit:ios": "npm run eas:submit -- --platform ios",
    "eas:submit:android": "npm run eas:submit -- --platform android",
    "codemagic:setup": "node scripts/setup-codemagic.js",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:report": "playwright show-report",
    "test:install": "playwright install",
    "build:optimized": "NODE_ENV=production next build",
    "build:mobile:optimized": "NODE_ENV=production npm run build:mobile",
    "start:optimized": "NODE_ENV=production next start",
    "lint:fix": "next lint --fix",
    "clean": "rm -rf .next out node_modules/.cache",
    "precommit": "npm run lint:fix && npm run type-check",
    "screenshots:install": "playwright install --with-deps",
    "screenshots:login": "node scripts/save-storage-state.js",
    "screenshots:appstore": "node scripts/capture-app-store-screenshots.js",
    "screenshots:all": "npm run screenshots:install && npm run screenshots:appstore",
    "images:fit": "node scripts/process-app-store-image.js",
    "images:fit:folder": "node scripts/process-app-store-image.js --in app-store-assets"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.450.0",
    "@aws-sdk/s3-request-presigner": "^3.450.0",
    "@capacitor/android": "^7.0.3",
    "@capacitor/app": "^7.0.3",
    "@capacitor/browser": "^7.0.2",
    "@capacitor/cli": "^7.0.3",
    "@capacitor/core": "^7.0.3",
    "@capacitor/device": "^8.0.0",
    "@capacitor/ios": "^7.0.3",
    "@capacitor/keyboard": "^7.0.3",
    "@capacitor/push-notifications": "^8.0.0",
    "@capacitor/splash-screen": "^7.0.3",
    "@capacitor/status-bar": "^7.0.3",
    "@capgo/capacitor-native-biometric": "^7.5.1",
    "@headlessui/react": "^2.2.9",
    "@hookform/resolvers": "^5.2.1",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/supabase-js": "^2.38.4",
    "@tanstack/react-query": "^5.85.6",
    "@tanstack/react-query-devtools": "^5.85.6",
    "@types/node": "^20.8.10",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "@types/react-window": "^1.8.8",
    "apexcharts": "^5.3.2",
    "autoprefixer": "^10.4.16",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "date-fns": "^2.30.0",
    "dotenv": "^17.2.1",
    "eslint": "^8.52.0",
    "eslint-config-next": "^13.5.4",
    "file-saver": "^2.0.5",
    "firebase-admin": "^13.6.0",
    "formik": "^2.4.5",
    "framer-motion": "^12.23.22",
    "lucide-react": "^0.292.0",
    "next": "15.5.9",
    "next-pwa": "5.6.0",
    "next-themes": "^0.4.6",
    "nodemailer": "^7.0.6",
    "postcss": "^8.4.31",
    "react": "18.2.0",
    "react-apexcharts": "^1.7.0",
    "react-datepicker": "^8.4.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.62.0",
    "react-hot-toast": "^2.4.1",
    "react-window": "^2.1.1",
    "react-window-infinite-loader": "^2.0.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "yup": "^1.3.3",
    "zod": "^4.0.15"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^13.5.11",
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/archiver": "^6.0.3",
    "@types/file-saver": "^2.0.7",
    "@types/jest": "^30.0.0",
    "@types/nodemailer": "^7.0.1",
    "@types/supertest": "^6.0.3",
    "husky": "^8.0.3",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "lint-staged": "^14.0.1",
    "msw": "^2.11.3",
    "next-test-api-route-handler": "^5.0.3",
    "playwright": "^1.55.0",
    "prettier": "^3.0.3",
    "sharp": "^0.33.5",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.4"
  }
}
