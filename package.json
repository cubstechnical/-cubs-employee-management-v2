{
  "name": "cubs-admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "build:analyze": "set ANALYZE=true && npm run build",
    "analyze:bundle": "node scripts/analyze-bundle.js",
    "test:performance": "node scripts/performance-test.js",
    "test:performance:mobile": "node scripts/performance-test-mobile.js",
    "test:performance:budget": "node scripts/performance-budget.js",
    "performance:check": "npm run build && npm run test:performance:budget",
    "performance:monitor": "node -e \"require('./lib/monitoring/performance').initializePerformanceMonitoring()\"",
    "db:migrate": "supabase db push",
    "db:indexes": "supabase db push --include-all",
    "optimize": "npm run performance:check && npm run db:indexes",
    "test:email": "node scripts/test-email.js",
    "setup:notifications": "node scripts/setup-notifications.js",
    "optimize:bg-image": "node scripts/optimize-bg-image.js",
    "quick-optimize": "node scripts/quick-optimize-bg.js",
    "test:unit": "jest --testPathPatterns=__tests__/unit",
    "test:integration": "jest --testPathPatterns=__tests__/integration",
    "test:all": "jest",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:ci": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:production": "node scripts/run-production-tests.js",
    "build:mobile": "node scripts/build-mobile.js",
    "build:mobile:android": "npm run build:mobile && npx cap open android",
    "build:mobile:ios": "npm run build:mobile && cap open ios",
    "build:ios": "npm run build && npx cap sync ios",
    "build:ios:clean": "rm -rf out && npm run build:ios",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "generate-icons": "node scripts/generate-app-icons.js",
    "generate-metadata": "node scripts/generate-app-store-metadata.js",
    "setup-demo-accounts": "node scripts/setup-demo-accounts.js",
    "prepare-app-store": "npm run generate-icons && npm run generate-metadata",
    "cap": "cap",
    "cap:add:ios": "cap add ios",
    "cap:add:android": "cap add android",
    "cap:build": "npm run build:mobile",
    "cap:ios": "cap open ios",
    "cap:android": "cap open android",
    "android:build": "npm run cap:build && cd android && ./gradlew assembleDebug",
    "android:run": "npm run cap:build && cd android && ./gradlew runDebug",
    "ios:build": "npm run cap:build && cap open ios",
    "eas:build": "eas build",
    "eas:build:ios": "eas build --platform ios",
    "eas:build:android": "eas build --platform android",
    "eas:build:ios:production": "eas build --platform ios --profile production",
    "eas:build:android:production": "eas build --platform android --profile production",
    "eas:build:preview": "eas build --profile preview",
    "eas:submit": "eas submit",
    "eas:submit:ios": "npm run eas:submit -- --platform ios",
    "eas:submit:android": "npm run eas:submit -- --platform android",
    "eas:setup": "node scripts/setup-eas-build.js",
    "eas:configure:ios": "node scripts/configure-ios-credentials.js",
    "eas:verify": "node scripts/verify-eas-build.js",
    "codemagic:setup": "node scripts/setup-codemagic.js",
    "publishing:setup": "node scripts/setup-automated-publishing.js",
    "verify:app-store": "node scripts/verify-app-store-readiness.js",
    "verify:build": "node scripts/verify-build.js",
    "verify:final": "node scripts/verify-build.js",
    "verify:all": "npm run verify:app-store && npm run verify:build && npm run verify:final",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:report": "playwright show-report",
    "test:install": "playwright install"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.450.0",
    "@aws-sdk/s3-request-presigner": "^3.450.0",
    "@capacitor/android": "^7.0.3",
    "@capacitor/app": "^7.0.3",
    "@capacitor/browser": "^7.0.2",
    "@capacitor/cli": "^7.0.3",
    "@capacitor/core": "^7.0.3",
    "@capacitor/ios": "^7.0.3",
    "@capacitor/keyboard": "^7.0.3",
    "@capacitor/splash-screen": "^7.0.3",
    "@capacitor/status-bar": "^7.0.3",
    "@hookform/resolvers": "^5.2.1",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/supabase-js": "^2.38.4",
    "@tanstack/react-query": "^5.85.6",
    "@tanstack/react-query-devtools": "^5.85.6",
    "@types/node": "^20.8.10",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "@types/react-window": "^1.8.8",
    "apexcharts": "^5.3.2",
    "archiver": "^5.3.1",
    "autoprefixer": "^10.4.16",
    "aws-sdk": "^2.1692.0",
    "backblaze-b2": "^1.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "csv-parse": "^6.1.0",
    "date-fns": "^2.30.0",
    "dotenv": "^17.2.1",
    "eslint": "^8.52.0",
    "eslint-config-next": "^13.5.4",
    "exceljs": "^4.4.0",
    "expo": "~50.0.0",
    "expo-splash-screen": "~0.26.0",
    "expo-status-bar": "~1.11.1",
    "file-saver": "^2.0.5",
    "firebase-admin": "^13.4.0",
    "formik": "^2.4.5",
    "framer-motion": "^12.23.18",
    "jszip": "^3.10.1",
    "lucide-react": "^0.292.0",
    "next": "^15.5.3",
    "next-pwa": "5.6.0",
    "next-themes": "^0.4.6",
    "nodemailer": "^7.0.5",
    "postcss": "^8.4.31",
    "react": "18.2.0",
    "react-apexcharts": "^1.7.0",
    "react-datepicker": "^8.4.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.62.0",
    "react-hot-toast": "^2.4.1",
    "react-native": "0.73.6",
    "react-native-web": "~0.19.6",
    "react-window": "^2.1.1",
    "react-window-infinite-loader": "^2.0.0",
    "string-similarity": "^4.0.4",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "yup": "^1.3.3",
    "zod": "^4.0.15"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^13.5.11",
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/archiver": "^6.0.3",
    "@types/file-saver": "^2.0.7",
    "@types/jest": "^30.0.0",
    "@types/nodemailer": "^7.0.1",
    "@types/supertest": "^6.0.3",
    "husky": "^8.0.3",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "lint-staged": "^14.0.1",
    "msw": "^2.11.3",
    "next-test-api-route-handler": "^5.0.3",
    "playwright": "^1.55.0",
    "prettier": "^3.0.3",
    "sharp": "^0.33.5",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.4"
  }
}
